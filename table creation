CREATE TABLE DEPT (
    DEPTNO NUMBER(2) NOT NULL,
    DNAME  VARCHAR2(14),
    LOC    VARCHAR2(13)
);
DESCRIBE dept
INSERT INTO DEPT VALUES (10, 'ACCOUNTING', 'NEW YORK'),
(20, 'RESEARCH',   'DALLAS'),
(30, 'SALES',      'CHICAGO'),
(40, 'OPERATIONS', 'BOSTON');
