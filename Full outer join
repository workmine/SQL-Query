CREATE TABLE Student1 (
    student_id INT PRIMARY KEY,
    name VARCHAR(50)
);
CREATE TABLE Marks1 (
    mark_id INT PRIMARY KEY,
    student_id INT,
    marks INT
);
INSERT INTO Student1 VALUES
(1, 'Amit'),
(2, 'Suman'),
(3, 'Rahul');
INSERT INTO Marks1 VALUES
(101, 1, 85),
(102, 2, 90),
(103, 4, 75);
SELECT Student1.student_id,
       Student1.name,
       Marks1.marks
FROM Student1
FULL OUTER JOIN Marks1
ON Student1.student_id = Marks1.student_id;
